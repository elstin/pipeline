{
    "name": "sanmai/pipeline",
    "description": "General purpose collections pipeline",
    "type": "library",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require": {
        "php": ">=7.1"
    },
    "require-dev": {
        "codacy/coverage": "^1.4",
        "friendsofphp/php-cs-fixer": "^2.11",
        "infection/infection": "~0.8",
        "league/pipeline": "^1.0|^0.3",
        "mockery/mockery": "^1.0",
        "phan/phan": "^0.12",
        "php-coveralls/php-coveralls": "^2.0",
        "phpstan/phpstan": "~0.9",
        "phpunit/phpunit": ">=6",
        "vimeo/psalm": ">=1.1"
    },
    "license": "Apache-2.0",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {"Pipeline\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"Tests\\Pipeline\\": "tests/"}
    },
    "extra": {
        "branch-alias": {
            "dev-legacy": "1.x-dev",
            "dev-master": "2.x-dev",
            "dev-future": "3.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
