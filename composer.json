{
    "name": "sanmai/pipeline",
    "type": "library",
    "description": "General-purpose collections pipeline for PHP",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require": {
        "php": "^5.6"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^2.15",
        "php-coveralls/php-coveralls": ">=1.0",
        "phpunit/phpunit": "^5"
    },
    "config": {
        "platform": {
            "php": "5.6"
        },
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Pipeline\\": "src/Pipeline/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Pipeline\\": "tests/Pipeline/"
        }
    }
}
