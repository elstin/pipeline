{
    "name": "sanmai/pipeline",
    "description": "Not your general purpose collection pipeline for PHP",
    "type": "library",
    "authors": [
        {
            "name": "Alexey Kopytko",
            "email": "alexey@kopytko.com"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5",
        "satooshi/php-coveralls": "^1.0",
        "friendsofphp/php-cs-fixer": "^2.4"
    },
    "license": "Apache-2.0",
    "autoload": {
        "files": ["src/functions.php"],
        "psr-4": {"Pipeline\\": "src/Pipeline/"}
    },
    "autoload-dev": {
        "psr-4": {"Pipeline\\": "tests/Pipeline/"}
    },
    "require": {
        "php": "^5.6"
    },
    "extra": {
        "branch-alias": {
            "dev-prehistoric": "v0.3.x-dev",
            "dev-legacy": "1.x-dev",
            "dev-master": "2.x-dev",
            "dev-future": "3.x-dev"
        }
    },
    "config": {
        "platform": {
            "php": "5.6"
        },
        "sort-packages": true
    }
}
